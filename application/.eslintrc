{
    "extends": [
      "./node_modules/gts",
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "lines-between-class-members" : ["error", "always", {"exceptAfterSingleLine": true} ],
      "semi": 0,
      "react/jsx-filename-extension": [1,
            { "extensions": [".ts", ".tsx"] }
        ],
      "prettier/prettier": [
        "error", {
          "semi": false
        }
      ]
    }
  }
